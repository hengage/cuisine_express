<h2>Your reservations</h2>

<% if (reservations) {%>
    <% reservations.forEach(reservation => { %>
        <ul>
            <li>
                <%= reservation.code %>
                <%= reservation.user %>
                <%= reservation.restaurant %>
            </li>
        </ul>
    <% }) %>
<% } else { %> 
    <p>You have no table reservation</p>
<% } %>

